{
  "ConnectionStrings": {
    "InternaryDatabaseConnection": "Server=.\\SQLEXPRESS;Database=InternaryDatabase;Trusted_Connection=True;"
  },

  "CorsAllowedOrigins": [
    "http://localhost:3000",
    "https://ele-bo.azurewebsites.net",
    "https://elephants-backoffice.azurewebsites.net"
  ],
  
  "Logging": {
    "LogLevel": {
        "Default": "Error",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning"
    },
    "Debug": {
        "LogLevel": {
            "Default": "Information"
        }
    },
    "Console": {
        "IncludeScopes": true,
        "LogLevel": {
            "Microsoft.Extensions.Hosting": "Warning",
            "Default": "Information"
        }
    },
    "EventSource": {
        "LogLevel": {
            "Microsoft": "Information"
        }
    },
    "EventLog": {
        "LogLevel": {
            "Microsoft": "Information"
        }
    },
    "AzureAppServicesFile": {
        "IncludeScopes": true,
        "LogLevel": {
            "Default": "Warning"
        }
    },
    "AzureAppServicesBlob": {
        "IncludeScopes": true,
        "LogLevel": {
            "Microsoft": "Information"
        }
    },
    "ApplicationInsights": {
        "LogLevel": {
            "Default": "Information"
        }
    }
  },
  "AllowedHosts": "*"
}
